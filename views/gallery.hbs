<!DOCTYPE html>
<html>
<head>
    {{> header}}
    <link rel="stylesheet" type="text/css" href="/css/gallery.css" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>

<div class="main-wrapper">
    <!-- nav -->
    <div class="sticky">
    {{> navBar}}
    </div>
    <!-- /nav -->
    <!-- Page content begin-->
    <div class="content">
        <!-- slide show -->
        <div class="gallery">
            <div class="slideshow-container">
                <div class="mySlides fade">
                    <img src="http://res.cloudinary.com/miaocloud/image/upload/v1509318389/pic04_wnxr3v.jpg" style="width:100%;">
                </div>
                <div class="mySlides fade">
                    <img src="http://res.cloudinary.com/miaocloud/image/upload/v1509318349/pic05_wjfoz5.jpg" style="width:100%;">
                </div>

                <div class="mySlides fade">
                    <img src="http://res.cloudinary.com/miaocloud/image/upload/v1509318393/pic06_rnzrl2.jpg" style="width:100%;">
                </div>
                <div class="mySlides fade">
                    <img src="http://res.cloudinary.com/miaocloud/image/upload/v1509318347/pic07_ehthm8.jpg" style="width:100%;">
                </div>

                <div class="mySlides fade">
                    <img src="http://res.cloudinary.com/miaocloud/image/upload/v1509318368/pic08_nzv3q2.jpg" style="width:100%;">
                </div>

                <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
                <a class="next" onclick="plusSlides(1)">&#10095;</a>
                <ul class="dotul">
                   <li><span class="dot" onclick="currentSlide(1)"></span> </li>
                   <li><span class="dot" onclick="currentSlide(2)"></span> </li>
                   <li><span class="dot" onclick="currentSlide(3)"></span> </li>
                   <li><span class="dot" onclick="currentSlide(4)"></span> </li>
                   <li><span class="dot" onclick="currentSlide(5)"></span> </li>
                </ul>
            </div>
        </div>
        <!-- slide show end -->
        <section class="circle-section">
            <div class="heading-wrapper">
                <h2>Gallery</h2>
                <div class="hr"> </div>
                <div class="sub-padding">Enjoy using Adobe to turn my thought into reality
                </div>
            </div>
            <section class="circles">
                <div class="circle">
                    <div class="card">
                        <h3><a href="/photos">Photo</a></h3>
                    </div>
                </div>
                <div class="circle">
                    <div class="card">
                        <h3><a href="/logo">Logo</a></h3>
                    </div>
                </div>
                <div class="circle">
                    <div class="card">
                        <h3><a href="/icon">Icon</a></h3>
                    </div>
                </div>
                <div class="circle">
                    <div class="card">
                        <h3><a href="/typegraphy">Typegraphy</a></h3>
                    </div>
                </div>
        </section>
        <div class="clearfix"></div>
        </section>
    </div>
    <!-- Page content end-->
    <!-- Page foot begin-->
    {{> footer}}
    <!-- Page foot end-->
</div>
<div class="overlap"></div>
<!--Side Bar -->
<div id="sidebar">
    {{> sideBar}}
</div>
<!--Go Top -->
<a href="#"><img class="backTotop" src="img/icon/top.png" /></a>

<script src="/js/gallery.js"></script>

</body>
</html>
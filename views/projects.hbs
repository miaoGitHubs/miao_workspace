            <div class="article-preview">
                <div class="text-section">
                    <div class="sub-heading">Vue.js single page app </div>
                </div>
                <div class="img-section">
                </div>
            </div>
            <div class="article-preview">
                <div class="img-section">

                </div>
                <div class="text-section">
                    <div class="sub-heading">React.js single page app </div>
                </div>
            </div>
            <div class="article-preview">
                <div class="text-section">
                    <div class="sub-heading">Angular.js single page app </div>
